    {{/if}}
  </nav>
  <main>
    <aside>
      <h2></h2>
    </aside>
    <div class="">
      {{{body}}}
    </div>
  </main>
    {{#if loggedIn}}
    <script src="/js/logout.js"></script>
    {{/if}}
    
    
</body>
